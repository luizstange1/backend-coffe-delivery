generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id              String  @id @default(uuid())
  name_product    String
  details_product String
  tag_product     String
  price_product   Decimal @default(0.00)
  image_path      String

  @@map("products")
}
